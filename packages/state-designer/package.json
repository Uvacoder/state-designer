{
  "name": "state-designer",
  "version": "1.6.5",
  "description": "State Designer, a state management library.",
  "author": "steveruizok",
  "repository": "https://github.com/steveruizok/state-designer",
  "private": false,
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch --tsconfig tsconfig.build.json --verbose --noClean",
    "build": "tsdx build --tsconfig tsconfig.build.json",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": "^17.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/react.esm.js",
  "size-limit": [
    {
      "path": "dist/react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.1",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "husky": "^5.1.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "size-limit": "^4.10.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@state-designer/core": "^1.6.5",
    "@state-designer/react": "^1.6.5"
  },
  "gitHead": "4c8cfd56c58c86d75ab4137bef03891fd95978f9"
}
