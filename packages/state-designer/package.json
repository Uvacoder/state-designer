    "husky": "^4.2.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "tsdx": "^0.13.2",
    "tslib": "^1.11.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@state-designer/core": "latest",
    "@state-designer/react": "latest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "env": {
    "test": {
      "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
